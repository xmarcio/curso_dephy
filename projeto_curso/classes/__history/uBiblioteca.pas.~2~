unit uBiblioteca;

interface

uses IniFiles, System.SysUtils, Vcl.Forms;

procedure SetValorIni(pLocal, pSessao, pSubSessao, pValor: string);

implementation

procedure SetValorIni(pLocal, pSessao, pSubSessao, pValor: string);
var
  vArquivo: TIniFile;
begin
  vArquivo := TIniFile.Create(ExtractFilePath(Application.ExeName));

end;

end.
